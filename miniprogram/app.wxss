/**app.wxss**/
@import "static/font/iconfont.wxss";
@import "colorui/main.wxss";
@import "colorui/icon.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 


.public-shadow1 {
  box-shadow: rgba(33, 33, 33, 0.12) 0px 0px 3px, rgba(33, 33, 33, 0.12) 0px 1px 3px;
}

.public-shadow2 {
  box-shadow: rgba(33, 33, 33, 0.12) 0px 0px 7px, rgba(33, 33, 33, 0.12) 0px 3px 5px;
}

.public-shadow3 {
  box-shadow: rgba(33, 33, 33, 0.12) 0px 0px 24px, rgba(33, 33, 33, 0.12) 0px 5px 7px;
}